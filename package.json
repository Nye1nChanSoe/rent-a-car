{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "prepare": "husky",
      "lint:fix": "./vendor/bin/phpstan && "
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "axios": "^1.7.4",
    "concurrently": "^9.0.1",
    "husky": "^9.1.7",
    "laravel-vite-plugin": "^1.2.0",
    "lint-staged": "^15.4.3",
    "tailwindcss": "^4.0.0",
    "vite": "^6.0.11"
  },
    "lint-staged": {
        "*.php": [
            "./vendor/bin/pint",
            "php -d memory_limit=512M ./vendor/bin/phpstan analyse --configuration=phpstan.dev.neon",
            "./vendor/bin/rector process --dry-run"
        ]
    }
}
